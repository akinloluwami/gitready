{
  "name": "gitready",
  "version": "1.0.0",
  "description": "A CLI tool to clone multiple GitHub repositories at once",
  "main": "index.js",
  "bin": {
    "gitpro": "./index.js"
  },
  "type": "module",
  "scripts": {
    "start": "node ./dist/index.js",
    "dev": "nodemon ./src/index.js",
    "build": "babel src --out-dir dist --copy-files",
    "clone": "node ./dist/index.js clone <username> --all"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "commander": "^8.3.0",
    "fs": "^0.0.1-security",
    "got": "^12.6.0",
    "got-fetch": "^5.1.4",
    "loading-cli": "^1.1.0",
    "node-fetch": "^3.3.1",
    "ora": "^6.1.2",
    "path": "^0.12.7",
    "simple-git": "^2.19.1",
    "terminal-multi-spinners": "^0.4.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.20.2"
  }
}
